    pass

def per_frame():
    fRating=4
    pe.gamma=2
    pe.decay=0.99
    echo_zoom=2.977723
    echo_alpha=0.5
    echo_orient=0
    pe.wave_mode=2
    pe.wave_additive=1
    pe.wave_usedots=1
    bModWaveAlphaByVolume=0
    pe.wave_brighten=1
    pe.wrap=1
    darken_center=0
    bMotionVectorsOn=0
    bRedBlueStereo=0
    pe.mv_x=12
    pe.mv_y=9
    brighten=0
    darken=0
    solarize=0
    pe.invert=0
    pe.wave_a=3.99624
    fWaveScale=1.497458
    fWaveSmoothing=0.7
    pe.wave_mystery=0
    fModWaveAlphaStart=0.75
    fModWaveAlphaEnd=0.95
    pe.warp_speed=1
    pe.warp_scale=2.162176
    pe.zoomexp=1.001
    fShader=1
    pe.zoom=1
