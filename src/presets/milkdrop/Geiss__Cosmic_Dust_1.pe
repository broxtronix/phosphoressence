    pe.cx=0.5
    pe.cy=0.5
    pe.dx=0
    pe.dy=0
    pe.warp=0.796896
    pe.sx=1
    pe.sy=1
    pe.wave_r=0.7
    pe.wave_g=0.7
    pe.wave_b=0.7
    pe.wave_x=0.5
    pe.wave_y=0.5
    pe.wave_x = pe.wave_x + 0.150*( 0.60*math.sin(2.121*pe.time) + 0.40*math.sin(1.621*pe.time) );
    pe.wave_y = pe.wave_y + 0.150*( 0.60*math.sin(1.742*pe.time) + 0.40*math.sin(2.322*pe.time) );
    pe.wave_r = pe.wave_r + 0.500*( 0.60*math.sin(0.823*pe.time) + 0.40*math.sin(0.916*pe.time) );
    pe.wave_g = pe.wave_g + 0.500*( 0.60*math.sin(0.900*pe.time) + 0.40*math.sin(1.023*pe.time) );
    pe.wave_b = pe.wave_b + 0.500*( 0.60*math.sin(0.808*pe.time) + 0.40*math.sin(0.949*pe.time) );
    pe.sx = pe.sx + 0.010*( 0.60*math.sin(0.313*pe.time) + 0.40*math.sin(0.383*pe.time) );
    pe.sy = pe.sy + 0.010*( 0.60*math.sin(0.367*pe.time) + 0.40*math.sin(0.272*pe.time) );
    pe.decay = pe.decay - 0.01*math.equal(pe.frame%20,0);

def per_pixel():
    pe.rot=pe.rot+pe.rad*0.18*math.sin(0.4*pe.time);

pe_presets.register(name, per_frame, initialize, per_pixel)
